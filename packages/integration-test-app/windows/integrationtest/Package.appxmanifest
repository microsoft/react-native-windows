<?xml version="1.0" encoding="utf-8"?>

<Package
  xmlns="http://schemas.microsoft.com/appx/manifest/foundation/windows10"
  xmlns:mp="http://schemas.microsoft.com/appx/2014/phone/manifest"
  xmlns:uap="http://schemas.microsoft.com/appx/manifest/uap/windows10"
  IgnorableNamespaces="uap mp">

  <Identity
    Name="integrationtest"
    Publisher="CN=react-native-windows-testing"
    Version="1.0.0.0" />

  <mp:PhoneIdentity PhoneProductId="7889084a-58b3-490f-9d1c-2abf27e2edb4" PhonePublisherId="00000000-0000-0000-0000-000000000000"/>

  <Properties>
    <DisplayName>integrationtest</DisplayName>
    <PublisherDisplayName>react-native-windows-testing</PublisherDisplayName>
